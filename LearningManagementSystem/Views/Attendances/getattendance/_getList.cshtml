@model IEnumerable<Attendance>

<div class="col-md-10 col-lg-12 col-md-offset-1 col-lg-offset-0">

    <div class="panel panel-default" id="listenrol">
        <div class="table-responsive">
            <table class="table v-middle">


                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Attendance</th>
                    </tr>
                </thead>


                @foreach (var item in Model)
                {
                    <tr>

                        <td><span class="label label-default">@Html.DisplayFor(model => item.Enrollment.Student.FullName)</span></td>
                        <td><span class="label label-default">@Html.DisplayFor(Model => item.AttendanceStatus)</span></td>
                    </tr>
                }
            </table>
            @*}*@
        </div>
    </div>
</div>